!to "TestACME.prg", cbm
!sal
!sl "TestACME.map"
!svl "TestACME.lbl"
!pdb "TestACME.pdb"
!cpu 6510
!ct pet

WoobleImm = $05
WoobleAddr = $06
WoobleImm2 = $07

* = $0400
	lda #WoobleImm
	lda WoobleAddr
WoobleAddrPC lda WoobleAddrPC
	lda #WoobleImm
	rts

